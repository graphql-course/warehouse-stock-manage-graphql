{
  "name": "stock-managemente",
  "version": "1.0.0",
  "description": "Gestión de almacén en tiempo real",
  "main": "build/app.js",
  "scripts": {
    "start": "node build/app.js",
    "build": "npx tsc -p . && ncp src/schema build/schema",
    "dev": "nodemon \"src/app.ts\" --exec \"ts-node\" \"src/app.ts\" -e ts,graphql,json",
    "publish": "npm run build && npx vercel --prod"
  },
  "keywords": [
    "graphql-from-scratch",
    "traffic",
    "json",
    "dgt"
  ],
  "homepage": "https://stock-management-graphql.herokuapp.com/",
  "author": "Anartz Mugika Ledo <mugan86@gmail.com> (anartz-mugika.com)",
  "license": "MIT",
  "dependencies": {
    "@graphql-tools/load-files": "^6.4.0",
    "@graphql-tools/schema": "^7.1.5",
    "apollo-server-express": "^3",
    "bcrypt": "^5.0.1",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import-node": "^0.0.4",
    "graphql-subscriptions": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mongodb": "^4.1.2",
    "ncp": "^2.0.0",
    "node-machine-id": "^1.1.12",
    "subscriptions-transport-ws": "^0.9.19",
    "typescript": "^4.4.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1"
  }
}
